<program>     ::= <def-or-expr>+
<def-or-expr> ::= <definition> | <expression>

<definition>  ::= ( define <ident> <expression> )

<expression>  ::= ( <expression> <expression>+ )
			    | <ident>
			    | <value>

<value>       ::= ( lambda ( <ident>+ ) <expression> )
                | <integer>
			    | <float>
			    | <boolean>
			    | <string>
			    | <primitive>
